            "device": "STM32F303CC",
            "serverpath": "/opt/SEGGER/JLink/JLinkGDBServerCLExe",
            "servertype": "jlink",
            "interface": "swd",
            "runToMain": true,
            ""
            ,
            "armToolchainPath": "/usr/bin/",
            "gdbPath": "gdb-multiarch",
            "preLaunchTask": "make"
        },
        {
            "type": "cortex-debug",
            "request": "attach",
            "name": "Debug attach ARM YAEMK J-Link",
            "cwd": "${workspaceRoot}",
            "executable": ".build/karlk90_yaemk_koy.elf",
            "device": "STM32F303CC",
            "serverpath": "/opt/SEGGER/JLink/JLinkGDBServerCLExe",
            "servertype": "jlink",
            "interface": "swd",
            "armToolchainPath": "/usr/bin/",
            "gdbPath": "gdb-multiarch",
            "preLaunchTask": "make"
        },
        {
            "type": "cortex-debug",
            "request": "launch",
            "name": "Debug launch RISC-V YAEMK J-Link",
            "cwd": "${workspaceRoot}",
            "executable": ".build/karlk90_yaemk_koy.elf",
            "device": "GD32VF103CBT6",
            "serverpath": "/opt/SEGGER/JLink/JLinkGDBServerCLExe",
            "servertype": "jlink",
            "interface": "jtag",
            "runToMain": true,
            "armToolchainPath": "/usr/bin/",
            "gdbPath": "riscv64-unknown-elf-gdb",
            "preLaunchTask": "make"
        },
        {
            "type": "cortex-debug",
